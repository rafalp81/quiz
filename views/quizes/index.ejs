<form method="get" action="/quizes">
    <input type="text" name="search" placeholder="Buscar pregunta" />
    <input type="submit" value="Enviar" />
</form>
<table>
  <% for(var i=0; i< quizes.length; i++){ %>
        <tr>
          <td>
            <a href="quizes/<%=quizes[i].id %>"><%=quizes[i].pregunta%></a>
          </td>
        </tr>
  <% } %>
</table>
